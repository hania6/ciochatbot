{
  "name": "MERN-boilerplate",
  "version": "1.0.0",
  "description": "MERN-R stack project boilerplate with Styled-Components, Forked from ARKON's MERN-Boilereplate",
  "author": "Ahmed Saad",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Legendofire/MERN-boilerplate"
  },
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "8.11.4"
  },
  "nodemonConfig": {
    "ignore": [
      "client/*",
      "docs/*",
      "tests/*"
    ]
  },
  "scripts": {
    "start": "node server.js",
    "start:dev": "nodemon server",
    "build": "webpack -p --progress --profile --colors",
    "deploy": "webpack && bx app push CIOChatBot",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@livechat/ui-kit": "^0.1.0",
    "axios": "^0.18.0",
    "body-parser": "^1.17.2",
    "cloudant": "^1.10.0-NOTICE",
    "connect-history-api-fallback": "^1.3.0",
    "express": "^4.15.4",
    "mongoose": "^4.11.7",
    "nodemon": "^1.11.0",
    "react": "^15.6.1",
    "react-chat-elements": "^0.9.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "recharts": "^1.0.0-beta.10",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "remote-redux-devtools": "^0.5.12",
    "scroll-into-view-if-needed": "^2.1.5",
    "styled-components": "^3.2.6",
    "watson-developer-cloud": "^3.4.0"
